    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "axios": "1.3.5",
    "framer-motion": "10.12.4",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.2.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.0",
